{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "String",
      "required": true,
      "minlength": 2,
      "maxlength": 50
    },
    "lastName": {
      "type": "String",
      "required": true,
      "minlength": 2,
      "maxlength": 50
    },
    "location": {
      "type": "String",
      "required": true
    },
    "nativeLanguage": {
      "type": "String",
      "required": true
    },
    "languageToLearn": {
      "type": "String",
      "required": true
    },
    "phoneNumber": {
      "type": "Number",
      "required": true,
      "minlength": 2,
      "maxlength": 10
    },
    "Gender": {
      "type": "String",
      "required": true
    },
    "email": {
      "type": "String",
      "required": true,
      "index": true,
      "minlength": 5,
      "maxlength": 255,
      "unique": true,
      "trim": true
    },
    "password": {
      "type": "String",
      "required": true,
      "minlength": 6,
      "maxlength": 16,
      "trim": true
    },
    "birthdate": {
      "type": "Date",
      "require": true,
      "trim": true
    },
    "skill": {
      "type": "String",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "Profile": {
      "type": "hasOne",
      "model": "Profile",
      "foreignKey": ""
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "CustomAccessToken",
      "foreignKey": "userId"
    },
    "Post": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": ""
    },
    "PostImage": {
      "type": "hasMany",
      "model": "PostImage",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
