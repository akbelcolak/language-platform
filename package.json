  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "jest": "^25.2.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
